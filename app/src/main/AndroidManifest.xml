<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.happybirthday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HappyBirthday">
        <activity
            android:name=".ComposeQuadrant"
            android:exported="false"
            android:label="@string/title_activity_compose_quadrant"
            android:theme="@style/Theme.HappyBirthday" >

        </activity>
        <activity
            android:name=".TaskManager"
            android:exported="false"
            android:label="@string/title_activity_task_manager"
            android:theme="@style/Theme.HappyBirthday">

        </activity>
        <activity
            android:name=".ComposeArticle"
            android:exported="false"
            android:label="@string/title_activity_compose_article"
            android:theme="@style/Theme.HappyBirthday"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.HappyBirthday">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        </activity>
    </application>

</manifest>